{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "node",
            "request": "launch",
            "name": "DiscordBot",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/DiscordBot/main.js",
            "cwd": "${workspaceFolder}/DiscordBot"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "ShardManager",
            "args": ["localhost"],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/shardManager/main.js",
            "cwd": "${workspaceFolder}/shardManager"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "WebsiteBackend",
            "args": ["localhost", "3000", "testtest123"],
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/WebsiteBackend/GameServer/server.js",
            "cwd": "${workspaceFolder}/WebsiteBackend"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "LoginWebsite",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/LoginWebsite/main.js",
            "cwd": "${workspaceFolder}/LoginWebsite"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Scheduler",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/Scheduler/script/main.js",
            "cwd": "${workspaceFolder}/Scheduler"
        },
        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "MapMaker",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/MapMaker/"
        }
    ]
}